t += 1
call pump2stretch()
%pulseon: tweezerIntensityTrig 0.1
%dac: toppulselev top_raman_carrier__servo_value
dac: toppulselev 0
dac: axPulseLev ax_raman_spec__servo_value
dac: eopulselev eo_raman__servo_value
rsg: (6.83103 + uw_spec_mod_khz/1000000) 5
var pulsetime 0.06
on: axialRamanRf
on: eoramanrf
on: eoswitch
on: eoswitch2
on: a15
t += pulsetime
off: a15
off: axialRamanRf
off: eoramanRF
off: eoswitch
off: eoswitch2
off: axialRamanShutter
off: axialRamanShutter2
t += 1
call pushout()
pulseon: tweezerIntensityTrig 0.1
t += 5
pulseon: tweezerIntensityTrig 0.1
t += 1
pulseon: rsTrig 0.1
call pgcAtomFlashingImage()
off: axialRamanShutter
off: axialRamanShutter2
pulseon: tweezerIntensityTrig 0.1
off: eoramanrf
t += 0.2

